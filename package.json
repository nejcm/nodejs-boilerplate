{
  "name": "@nejcm/nodejs-boilerplate",
  "version": "0.0.0-semantically-released",
  "description": "Starter kit for nodejs projects",
  "author": "Nejc Mursic <nmursi2@gmail.com> (https://github.com/nejcm)",
  "license": "MIT",
  "main": "index.ts",
  "engines": {
    "node": "12.18.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nejcm/nodejs-boilerplate"
  },
  "bugs": {
    "url": "https://github.com/nejc/nodejs-boilerplate/issues"
  },
  "homepage": "https://github.com/nejcm/nodejs-boilerplate",
  "scripts": {
    "start": "node .",
    "build": "babel --delete-dir-on-start --out-dir dist --extensions \".ts,.js\" --copy-files --ignore \"**/__tests__/**,**/__mocks__/**\" --no-copy-ignored src",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "test:coverage": "npm run test -- --coverage",
    "format": "prettier --write \"**/*.+(ts|js|json|css|md|mdx|html)\"",
    "lint": "eslint .",
    "setup": "node install && npm run validate",
    "validate": "npm run lint && npm run test:coverage",
    "semantic-release": "semantic-release",
    "tsc": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate"
    }
  },
  "dependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.7",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@typegoose/typegoose": "^7.3.1",
    "argon2": "^0.26.2",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "bull": "^3.16.0",
    "celebrate": "^12.1.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cpy": "^8.1.0",
    "csurf": "^1.11.0",
    "dotenv": "^8.2.0",
    "event-dispatch": "^0.4.1",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-jwt": "^6.0.0",
    "helmet": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "mongoose": "^5.9.27",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.11",
    "rate-limiter-flexible": "^2.1.10",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.1.1",
    "typedi": "^0.8.0",
    "typescript": "^3.9.7",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.0",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@shelf/jest-mongodb": "^1.2.1",
    "@types/axios": "^0.14.0",
    "@types/bull": "^3.14.0",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.7",
    "@types/express-jwt": "0.0.42",
    "@types/helmet": "0.0.47",
    "@types/jest": "^26.0.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/loglevel": "^1.6.3",
    "@types/method-override": "0.0.31",
    "@types/mongoose": "^5.7.34",
